{
  "name": "svg2jsx",
  "version": "0.1.0",
  "description": "transform svg as jsx constants elements",
  "main": "lib/index.js",
  "config": {
    "MOCHA_OPTS": "-R dot --compilers js:babel-register src/__tests__/*.spec.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "npm run lint && npm run ut && npm run build && npm run webpack",
    "ut": "mocha $npm_package_config_MOCHA_OPTS",
    "webpack": "webpack --config ./examples/webpack.config",
    "build": "rm -rf lib && babel src/ --ignore __*__ --out-dir lib/",
    "release": "npm run build && npm publish",
    "lint": "eslint src/"
  },
  "author": "",
  "license": "WTFPL",
  "devDependencies": {
    "babel-cli": "6.5.x",
    "babel-eslint": "5.0.x",
    "babel-plugin-transform-class-properties": "6.5.x",
    "babel-plugin-transform-object-rest-spread": "6.5.x",
    "babel-preset-es2015": "6.5.x",
    "babel-register": "6.5.x",
    "chai": "3.5.x",
    "eslint": "1.10.x",
    "eslint-config-airbnb": "5.0.x",
    "eslint-plugin-react": "3.16.1",
    "mocha": "2.4.x",
    "react": "0.14.x",
    "react-dom": "0.14.x",
    "svgo": "0.6.x",
    "webpack": "1.12.x"
  },
  "dependencies": {
    "babel-core": "6.5.x",
    "babel-plugin-transform-react-jsx": "6.5.x",
    "cheerio": "0.20.x",
    "lodash": "4.5.x"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morlay/svg2jsx.git"
  },
  "bugs": {
    "url": "https://github.com/morlay/svg2jsx/issues"
  },
  "homepage": "https://github.com/morlay/svg2jsx#readme"
}
